google {
    lifeSciences.bootDiskSize = 10.GB
    lifeSciences.preemptible = true
    zone = 'us-east1-b'
    network = 'jax-poc-lifebit-01-vpc-network'
    subnetwork = 'us-east1-sub2'
}

executor {
    name = 'google-lifesciences'
}

params {
    accession = false
    type = "cram"
    loc = "gs.us"
    key_file  = false
}

profiles {  
    docker { docker.enabled = true }
}